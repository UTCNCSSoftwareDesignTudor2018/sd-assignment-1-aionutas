<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.CubicCurve?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="487.0" prefWidth="605.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.TeacherController">
   <children>
      <CubicCurve controlX1="-50.0" controlX2="50.0" controlY1="-100.0" controlY2="100.0" endX="150.0" fill="#ffbd1f" layoutX="312.0" layoutY="443.0" startX="-150.0" stroke="BLACK" />
      <Button fx:id="editButton" alignment="CENTER" layoutX="116.0" layoutY="140.0" mnemonicParsing="false" onAction="#editAccount" prefHeight="48.0" prefWidth="396.0" text="Edit Account" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="enrollButton" alignment="CENTER" layoutX="118.0" layoutY="207.0" mnemonicParsing="false" onAction="#deleteStudent" prefHeight="48.0" prefWidth="396.0" text="Remove Student Account">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="seeEnrollments" alignment="CENTER" layoutX="117.0" layoutY="278.0" mnemonicParsing="false" onAction="#seeStudentList" prefHeight="48.0" prefWidth="396.0" text="See University Students List">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Label fx:id="lblStatus" alignment="CENTER" contentDisplay="CENTER" layoutX="60.0" layoutY="56.0" prefHeight="39.0" prefWidth="506.0" textAlignment="CENTER" textFill="#274f87">
         <font>
            <Font name="System Bold Italic" size="21.0" />
         </font>
      </Label>
      <Button fx:id="addNewStudent" alignment="CENTER" layoutX="116.0" layoutY="353.0" mnemonicParsing="false" onAction="#createAccount" prefHeight="48.0" prefWidth="396.0" text="Add New Student">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
